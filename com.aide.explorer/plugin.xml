            <enablement>
               <or>
                  <objectClass
                        name="org.eclipse.jdt.internal.core.CompilationUnit">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.ui.part.FileEditorInput">
                  </objectClass>
               </or>
            </enablement>
         </action>
         <action
               class="com.aide.actions.CopyName2ClipBoardAction"
               enablesFor="1"
               id="com.aide.actions.CopyName2ClipBoardAction"
               label="Copy Path To ClipBoard...d..."
               menubarPath="aide/1">
            <enablement>
               <or>
                  <objectClass
                        name="org.eclipse.jdt.internal.core.CompilationUnit">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.jdt.internal.core.PackageFragment">
                  </objectClass>

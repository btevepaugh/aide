                  </objectClass>
               </or>
            </enablement>
         </action>
         <action
               class="com.aide.actions.BrowseDirAction"
               definitionId="com.aide.explorer.Browse"
               enablesFor="1"
               id="com.aide.actions.BrowseDirAction"
               label="Browse Directory..."
               menubarPath="aide/1">

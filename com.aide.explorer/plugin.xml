                  <objectClass
                        name="org.eclipse.jdt.internal.core.PackageFragmentRoot">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFolder">
                  </objectClass>
               </or>
            </enablement>
         </action>
         <action
               class="com.aide.actions.OpenCommandAction"
               definitionId="com.aide.explorer.command.OpenCommand"
               enablesFor="1"
               id="com.aide.actions.OpenCommandAction"
               label="Command..."
               menubarPath="aide/1">
            <enablement>
               <or>
                  <objectClass
                        name="org.eclipse.jdt.internal.core.CompilationUnit">
                  </objectClass>

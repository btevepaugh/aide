         </action>
         <menu
               id="aide"
               label="Aide Explorer">
            <groupMarker
                  name="0">
            </groupMarker>
            <separator
                  name="aide.separator1">
            </separator>
            <groupMarker
                  name="1">
            </groupMarker>
            <separator
                  name="aide.separator2">
            </separator>
            <groupMarker
